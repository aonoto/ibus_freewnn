<?xml version=\"1.0\" encoding=\"utf-8\"?>
<!-- filename: fwnn.xml -->
<component>
	<name>org.freedesktop.IBus.Fwnn</name>
	<description>FreeWnn Component</description>
	<exec>@IBUS_FWNN_LIB_DIR@/ibus-engine-fwnn --ibus</exec>
	<version>0.0.1</version>
	<author>Hashimoto Masahiko &lt;hashimom@geeko.jp&gt;</author>
	<license>GPL</license>
	<homepage>https://github.com/hashimom/ibus-fwnn</homepage>
	<textdomain>ibus-fwnn</textdomain>

	<engines>
		<engine>
			<name>fwnn</name>
			<language>ja</language>
			<license>GPL</license>
			<author>Hashimoto Masahiko &lt;hashimom@geeko.jp&gt;</author>
			<icon>@IBUS_FWNN_ROOT_DIR@/icons/ibus-fwnn.svg</icon>
			<layout>jp</layout>
			<longname>FreeWnn</longname>
			<description>FreeWnn Input Method</description>
			<rank>0</rank>
			<!-- symbol for iBus 1.4+ -->
			<!-- Currently set it to (non-standard in
			     Japanese IM) 'U' (in Hiragana), which
			     means first letter of Wnn in Hiragana
			     (pronunciation). -->
			<symbol>&#x3046;</symbol>
		</engine>
	</engines>

</component>
